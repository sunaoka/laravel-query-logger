{
    "name": "sunaoka/laravel-query-logger",
    "description": "Query logger for Laravel.",
    "keywords": ["laravel", "database", "logger", "log"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sunaoka",
            "email": "sunaoka@pocari.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.15 || ^11.0",
        "illuminate/support": "^10.15 || ^11.0",
        "symfony/console": "^6.0 || ^7.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9.8",
        "laravel/pint": "^1.17.3",
        "orchestra/testbench": "^8.26.0 || ^9.4.0"
    },
    "autoload": {
        "psr-4": {
            "Sunaoka\\LaravelQueryLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sunaoka\\LaravelQueryLogger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "Sunaoka\\LaravelQueryLogger\\QueryLoggerServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
