{
    "name": "sunaoka/laravel-query-logger",
    "description": "Query logger for Laravel.",
    "keywords": ["laravel", "database", "logger", "log"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sunaoka",
            "email": "sunaoka@pocari.org"
        }
    ],
    "require": {
        "php": "^7.1.3 || ^8.0",
        "illuminate/database": "^5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/support": "^5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8 || ^4.0 || ^5.0 || ^6.0 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Sunaoka\\LaravelQueryLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sunaoka\\LaravelQueryLogger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sunaoka\\LaravelQueryLogger\\QueryLoggerServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
